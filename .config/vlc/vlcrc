[param_eq] # 参数均衡器

# 低频 (Hz) (float)
#param-eq-lowf=100.000000

# 低频增益 (dB) (float)
#param-eq-lowgain=0.000000

# 高频 (Hz) (float)
#param-eq-highf=10000.000000

# 高频增益 (dB) (float)
#param-eq-highgain=0.000000

