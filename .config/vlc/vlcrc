#param-eq-f1=300.000000

# 频率 1 增益 (dB) (float)
#param-eq-gain1=0.000000

# 频率 1 Q (float)
#param-eq-q1=3.000000

# 频率 2 (Hz) (float)
#param-eq-f2=1000.000000

