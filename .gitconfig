[core]
    editor = vim
    # executable bit are ignored, useful for FAT.
    fileMode = false
    # convert CR/LF to LF only when fetching from remote repo
    autocrlf = input

    #symlinks = false
    #logallrefupdates = true

    ;gitproxy=""

[user]
    name  = Adaptee
    email = adaptee@gmail.com

[rerere]
    #enabled = true

[color]
    ui = auto

[diff]
    #renames = copy

[merge]
    tool = vimdiff

[status]
    relativePath = true

[alias]
    br      = branch
    cm      = commit -m 
    cma     = commit -a -m
    co      = checkout
    desc    = describe
    st      = status
    wtf     = whatchanged
    unstage = reset HEAD --
    last    = log -1 HEAD
    visual  = gitk

[i18n]
    commitEncoding    = utf-8
    logOutputEncoding = utf-8

[help]
    autocorrect = true
