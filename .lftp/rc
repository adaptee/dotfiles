debug 3

set ftp:charset "gbk"
set file:charset "UTF-8"
set bmk:save-passwords true

#set net:limit-rate 30000:0

set cls-default "--long --human-readable --classify --nocase --dirsfirst"

alias l  "cls --long --human-readable --classify --nocase --dirsfirst"

alias u "cd ../;l"
alias uu "cd ../../;l"
alias uuu "cd ../../../;l"
